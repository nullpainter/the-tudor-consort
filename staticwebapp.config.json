{
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  },
  "routes": [
    {
      "route": "/*",
      "allowedRoles": [
        "anonymous"
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://tudor-consort.org.nz/$1",
      "statusCode": 301
    }
  ]
}