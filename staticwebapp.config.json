{
  "routes": [
    {
      "route": "/*",
      "allowedRoles": [
        "anonymous"
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://tudor-consort.org.nz/$1",
      "statusCode": 301,
      "condition": {
        "hostname": "www.tudor-consort.org.nz"
      }
    }
  ]
}